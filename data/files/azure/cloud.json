  "credentials": [
    {
      "provider": "Azure",
      "displayFormat": "field",
      "annotations": {
        "pharmer.appscode.com/cluster-credential": "",
        "pharmer.appscode.com/dns-credential": ""
      },
      "fields": [
        {
          "envconfig": "AZURE_TENANT_ID",
          "form": "azure_tenant_id",
          "json": "tenantID",
          "label": "Tenant Id",
          "input": "text"
        },
        {
          "envconfig": "AZURE_SUBSCRIPTION_ID",
          "form": "azure_subscription_id",
          "json": "subscriptionID",
          "label": "Subscription Id",
          "input": "text"
        },
        {
          "envconfig": "AZURE_CLIENT_ID",
          "form": "azure_client_id",
          "json": "clientID",
          "label": "Client Id",
          "input": "text"
        },
        {
          "envconfig": "AZURE_CLIENT_SECRET",
          "form": "azure_client_secret",
          "json": "clientSecret",
          "label": "Client Secret",
          "input": "password"
        }
      ]
    },
    {
      "provider": "AzureStorage",
      "displayFormat": "field",
      "annotations": {
        "pharmer.appscode.com/storage-credential": ""
      },
      "fields": [
        {
          "envconfig": "AZURE_STORAGE_ACCOUNT",
          "form": "azure_storage_account",
          "json": "account",
          "label": "Azure Storage Account",
          "input": "text"
        },
        {
          "envconfig": "AZURE_STORAGE_KEY",
          "form": "azure_storage_key",
          "json": "key",
          "label": "Azure Storage Account Key",
          "input": "password"
        }
      ]
    }
  ],
  "kubernetesVersions": [
    {
      "version": "1.7.4",
      "description": "1.7.4",
      "env": {
        "dev": true
      },
      "defaultSpec": {
        "podSubnet": "10.244.0.0/16",
        "serviceSubnet": "10.0.0.0/16",
        "allocateNodeCidrs": true,
        "enableClusterMonitoring": "appscode",
        "enableClusterLogging": true,
        "enableNodeLogging": true,
        "loggingDestination": "appscode-elasticsearch",
        "elasticsearchLoggingReplicas": 1,
        "dnsServerIP": "10.0.0.10",
        "dnsDomain": "cluster.local",
        "admissionControl": "NamespaceLifecycle,LimitRanger,ServiceAccount,PersistentVolumeLabel,DefaultStorageClass,ResourceQuota",
        "masterSubnet": "10.246.0.0/24",
        "networkProvider": "flannel",
        "enableClusterAlert": "appscode",
        "os": "debian",
        "networkPolicyProvider": "none",
        "enableWebhookTokenAuthn": true,
        "enableRbacAuthz": true,
        "enableAPIserverBasicAudit": true,
        "enableAppscodeAttic": true,
        "pollSleepInterval": 3,
        "enableNodePublicIP": true
      }
    }
  ]
}